<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+0630: "ذ" ARABIC LETTER THAL
         U+06F1: "۱" EXTENDED ARABIC-INDIC DIGIT ONE -->
    <Key latin:keySpec="ذ" latin:additionalMoreKeys="۱,1" latin:keyHintLabel="۱"/>
    <!-- U+062F: "د" ARABIC LETTER DAL
         U+06F2: "۲" EXTENDED ARABIC-INDIC DIGIT TWO -->
    <Key latin:keySpec="د" latin:additionalMoreKeys="۲,2" latin:keyHintLabel="۲"/>
    <!-- U+062E: "خ" ARABIC LETTER KHAH
         U+06F3: "۳" EXTENDED ARABIC-INDIC DIGIT THREE -->
    <Key latin:keySpec="خ" latin:additionalMoreKeys="۳,3" latin:keyHintLabel="۳"/>
    <!-- U+062D: "ح" ARABIC LETTER HAH
         U+06F4: "۴" EXTENDED ARABIC-INDIC DIGIT FOUR -->
    <Key latin:keySpec="ح" latin:additionalMoreKeys="۴,4" latin:keyHintLabel="۴"/>
    <!-- U+0686: "چ" ARABIC LETTER TCHEH
         U+06F5: "۵" EXTENDED ARABIC-INDIC DIGIT FIVE -->
    <Key latin:keySpec="چ" latin:additionalMoreKeys="۵,5" latin:keyHintLabel="۵"/>
    <!-- U+062C: "ج" ARABIC LETTER JEEM
         U+06F6: "۶" EXTENDED ARABIC-INDIC DIGIT SIX -->
    <Key latin:keySpec="ج" latin:additionalMoreKeys="۶,6" latin:keyHintLabel="۶"/>
    <!-- U+062B: "ث" ARABIC LETTER THEH
         U+06F7: "۷" EXTENDED ARABIC-INDIC DIGIT SEVEN -->
    <Key latin:keySpec="ث" latin:additionalMoreKeys="۷,7" latin:keyHintLabel="۷"/>
    <!-- U+062A: "ت" ARABIC LETTER TEH
         U+0629: "ة": ARABIC LETTER TEH MARBUTA
         U+06F8: "۸" EXTENDED ARABIC-INDIC DIGIT EIGHT -->
    <Key latin:keySpec="ت" latin:moreKeys="ة" latin:additionalMoreKeys="۸,8" latin:keyHintLabel="۸"/>
    <!-- U+067E: "پ" ARABIC LETTER PEH
         U+06F9: "۹" EXTENDED ARABIC-INDIC DIGIT NINE -->
    <Key latin:keySpec="پ" latin:additionalMoreKeys="۹,9" latin:keyHintLabel="۹"/>
    <!-- U+0628: "ب" ARABIC LETTER BEH
         U+06F0: "۰" EXTENDED ARABIC-INDIC DIGIT ZERO -->
    <Key latin:keySpec="ب" latin:additionalMoreKeys="۰,0" latin:keyHintLabel="۰"/>
    <!-- U+0627: "ا" ARABIC LETTER ALEF
         U+0671: "ٱ" ARABIC LETTER ALEF WASLA
         U+0621: "ء" ARABIC LETTER HAMZA
         U+0622: "آ" ARABIC LETTER ALEF WITH MADDA ABOVE
         U+0623: "أ" ARABIC LETTER ALEF WITH HAMZA ABOVE
         U+0625: "إ" ARABIC LETTER ALEF WITH HAMZA BELOW -->
    <Key latin:keySpec="ا" latin:moreKeys="!fixedColumnOrder!5,ٱ,ء,آ,أ,إ"/>
</merge>
