<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <!-- U+0020: " " SPACE
                 U+0E39: " ู" THAI CHARACTER SARA UU -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ู|ู"/>
            <!-- U+0020: " " SPACE
                 U+0E3A: " ฺ" THAI CHARACTER PHINTHU -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ฺ|ฺ"/>
            <!-- U+0E3F: "฿" THAI CURRENCY SYMBOL BAHT -->
            <Key latin:keySpec="฿"/>
            <!-- U+0E42: "โ" THAI CHARACTER SARA O -->
            <Key latin:keySpec="โ"/>
            <!-- U+0020: " " SPACE
                 U+0E47: " ็" THAI CHARACTER MAITAIKHU -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ็|็"/>
            <!-- U+0020: " " SPACE
                 U+0E4A: " ๊" THAI CHARACTER MAI TRI -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ๊|๊"/>
            <!-- U+0020: " " SPACE
                 U+0E4B: " ๋" THAI CHARACTER MAI CHATTAWA -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ๋|๋"/>
            <!-- U+0020: " " SPACE
                 U+0E4C: " ์" THAI CHARACTER THANTHAKHAT -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ์|์"/>
            <!-- U+0020: " " SPACE
                 U+0E4D: " ํ" THAI CHARACTER THANTHAKHAT -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ํ|ํ"/>
        </case>
        <default>
            <!-- U+0020: " " SPACE
                 U+0E37: " ื" THAI CHARACTER SARA UEE -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ื|ื"/>
            <!-- U+0020: " " SPACE
                 U+0E38: " ุ" THAI CHARACTER SARA U -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ุ|ุ"/>
            <!-- U+0E40: "เ" THAI CHARACTER SARA E -->
            <Key latin:keySpec="เ"/>
            <!-- U+0E41: "แ" THAI CHARACTER SARA AE -->
            <Key latin:keySpec="แ"/>
            <!-- U+0E43: "ใ" THAI CHARACTER SARA AI MAIMUAN -->
            <Key latin:keySpec="ใ"/>
            <!-- U+0E44: "ไ" THAI CHARACTER SARA AI MAIMALAI -->
            <Key latin:keySpec="ไ"/>
            <!-- U+0E45: "ๅ" THAI CHARACTER LAKKHANGYAO -->
            <Key latin:keySpec="ๅ"/>
            <!-- U+0E46: "ๆ" THAI CHARACTER MAIYAMOK -->
            <Key latin:keySpec="ๆ"/>
            <!-- U+0020: " " SPACE
                 U+0E48: " ่" THAI CHARACTER MAI EK -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ่|่"/>
            <!-- U+0020: " " SPACE
                 U+0E49: " ้" THAI CHARACTER MAI THO -->
            <!-- Note: The space character is needed as a preceding letter to draw some Thai
                 composing characters correctly. -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec=" ้|้"/>
        </default>
    </switch>
</merge>
