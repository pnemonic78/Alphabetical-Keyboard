<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+0E9A: "ບ" LAO LETTER BO
         U+0E9B: "ປ" LAO LETTER PO -->
    <Key latin:keySpec="ບ" latin:moreKeys="ປ"/>
    <!-- U+0E9C: "ຜ" LAO LETTER PHO SUNG -->
    <Key latin:keySpec="ຜ"/>
    <!-- U+0E9D: "ຝ" LAO LETTER FO TAM
         U+0E9E: "ພ" LAO LETTER PHO TAM -->
    <Key latin:keySpec="ຝ" latin:moreKeys="ພ"/>
    <!-- U+0E9F: "ຟ" LAO LETTER FO SUNG -->
    <Key latin:keySpec="ຟ"/>
    <!-- U+0EA1: "ມ" LAO LETTER MO -->
    <Key latin:keySpec="ມ"/>
    <!-- U+0EA2: "ຢ" LAO LETTER YO -->
    <Key latin:keySpec="ຢ"/>
    <!-- U+0EA3: "ຣ" LAO LETTER LO LING
         U+0EAE: "ຮ" LAO LETTER HO TAM -->
    <Key latin:keySpec="ຣ" latin:moreKeys="ຮ"/>
    <!-- U+0EA5: "ລ" LAO LETTER LO LOOT -->
    <Key latin:keySpec="ລ"/>
    <!-- U+0EA7: "ວ" LAO LETTER WO -->
    <Key latin:keySpec="ວ"/>
    <!-- U+0EAA: "ສ" LAO LETTER SO SUNG -->
    <Key latin:keySpec="ສ"/>
    <!-- U+0EAB: "ຫ" LAO LETTER HO SUNG
         U+0EAB/U+0EBC: "ຫຼ" LAO LETTER HO SUNG/LAO SEMIVOWEL SIGN LO -->
    <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec="ຫ" latin:moreKeys="ຫຼ"/>
    <!-- U+0EAD: "ອ" LAO LETTER O -->
    <Key latin:keySpec="ອ"/>
    <!-- U+0EB0: "ະ" LAO VOWEL SIGN A -->
    <Key latin:keySpec="ະ"/>
</merge>
