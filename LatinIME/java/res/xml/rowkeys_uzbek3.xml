<merge xmlns:latin="http://schemas.android.com/apk/res-auto" >
    <Key latin:keySpec="v" latin:moreKeys="!text/morekeys_v"/>
    <Key latin:keySpec="x" latin:moreKeys="!text/morekeys_x"/>
    <Key latin:keySpec="y" latin:moreKeys="!text/morekeys_y"/>
    <Key latin:keySpec="z" latin:moreKeys="!text/morekeys_z"/>
    <!-- U+006F/U+02BB: "oʻ" LATIN SMALL LETTER O/MODIFIER LETTER TURNED COMMA -->
    <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec="oʻ"/>
    <!-- U+0067/U+02BB: "gʻ" LATIN SMALL LETTER G/MODIFIER LETTER TURNED COMMA -->
    <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec="gʻ"/>
    <!-- U+02BC: "ʼ" MODIFIER LETTER APOSTROPHE -->
    <Key latin:keySpec="ʼ"/>
</merge>