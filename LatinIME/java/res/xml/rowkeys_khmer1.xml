<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <!-- U+17B2: "ឲ" KHMER INDEPENDENT VOWEL QOO TYPE TWO
                 U+17B1: "ឱ" KHMER INDEPENDENT VOWEL QOO TYPE ONE -->
            <Key latin:keyHintLabel="ឱ" latin:keySpec="ឲ" latin:moreKeys="ឱ"/>
            <!-- U+17B6: "ា" KHMER VOWEL SIGN AA -->
            <Key latin:keySpec="ា"/>
            <!-- U+17B6/U+17C6: "ាំ" KHMER VOWEL SIGN AA/KHMER SIGN NIKAHIT -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec="ាំ"/>
            <!-- U+17B7: "ិ" KHMER VOWEL SIGN I -->
            <Key latin:keySpec="ិ"/>
            <!-- U+17B8: "ី" KHMER VOWEL SIGN II -->
            <Key latin:keySpec="ី"/>
            <!-- U+17B9: "ឹ" KHMER VOWEL SIGN Y -->
            <Key latin:keySpec="ឹ"/>
            <!-- U+17BA: "ឺ" KHMER VOWEL SIGN YY
                 U+17DD: "៝" KHMER SIGN ATTHACAN -->
            <Key latin:keyHintLabel="៝" latin:keyHintLabelVerticalAdjustment="40%" latin:keySpec="ឺ" latin:moreKeys="៝"/>
            <!-- U+17BB: "ុ" KHMER VOWEL SIGN U -->
            <Key latin:keySpec="ុ"/>
            <!-- U+17BB/U+17C6: "ុំ" KHMER VOWEL SIGN U/KHMER SIGN NIKAHIT -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec="ុំ"/>
            <!-- U+17BB/U+17C7: "ុះ" KHMER VOWEL SIGN U/KHMER SIGN REAHMUK -->
            <Key latin:keyLabelFlags="followKeyLetterRatio" latin:keySpec="ុះ"/>
        </case>
        <default>
            <!-- U+1780: "ក" KHMER LETTER KA -->
            <Key latin:keySpec="ក"/>
            <!-- U+1781: "ខ" KHMER LETTER KHA -->
            <Key latin:keySpec="ខ"/>
            <!-- U+1782: "គ" KHMER LETTER KO
                 U+179D: "ឝ" KHMER LETTER SHA -->
            <Key latin:keyHintLabel="ឝ" latin:keySpec="គ" latin:moreKeys="ឝ"/>
            <!-- U+1783: "ឃ" KHMER LETTER KHO -->
            <Key latin:keySpec="ឃ"/>
            <!-- U+1784: "ង" KHMER LETTER NGO -->
            <Key latin:keySpec="ង"/>
            <!-- U+1785: "ច" KHMER LETTER CA -->
            <Key latin:keySpec="ច"/>
            <!-- U+1786: "ឆ" KHMER LETTER CHA -->
            <Key latin:keySpec="ឆ"/>
            <!-- U+1787: "ជ" KHMER LETTER CO -->
            <Key latin:keySpec="ជ"/>
            <!-- U+1788: "ឈ" KHMER LETTER CHO
                 U+17DC: "ៜ" KHMER SIGN AVAKRAHASANYA -->
            <Key latin:keyHintLabel="ៜ" latin:keySpec="ឈ" latin:moreKeys="ៜ"/>
            <!-- U+1789: "ញ" KHMER LETTER NYO -->
            <Key latin:keySpec="ញ"/>
        </default>
    </switch>
</merge>
