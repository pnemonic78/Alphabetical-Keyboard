<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+20B9: "₹" INDIAN RUPEE SIGN
         U+00A3: "£" POUND SIGN
         U+00A4: "¤" CURRENCY SIGN
         U+0930+U+0941: "रु" NEPALESE RUPEE SIGN
         U+20A8: "₨" RUPEE SIGN
         U+20AC: "€" EURO SIGN
         U+00A2: "¢" CENT SIGN -->
    <key-style
        latin:styleName="currencyKeyStyle"
        latin:keyLabel="&#x20B9;"
        latin:keyHintLabel="&#x00A4;"
        latin:moreKeys="$,&#x00A2;,&#x00A3;,&#x00A4;,&#x20A8;,&#x20AC;,&#x0930;&#x0941;" />
    <key-style
        latin:styleName="moreCurrency1KeyStyle"
        latin:keyLabel="&#x00A3;" />
    <key-style
        latin:styleName="moreCurrency2KeyStyle"
        latin:keyLabel="&#x20AC;" />
    <key-style
        latin:styleName="moreCurrency3KeyStyle"
        latin:keyLabel="$"
        latin:keyHintLabel="&#x00A2;"
        latin:moreKeys="&#x00A2;" />
    <key-style
        latin:styleName="moreCurrency4KeyStyle"
        latin:keyLabel="&#x00A2;" />
</merge>
