<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <!-- U+0D8A: "ඊ" SINHALA LETTER IIYANNA -->
            <Key latin:keySpec="ඊ"/>
            <!-- U+0D8B: "උ" SINHALA LETTER UYANNA -->
            <Key latin:keySpec="උ"/>
            <!-- U+0D8D: "ඍ" SINHALA LETTER IRUYANNA -->
            <Key latin:keySpec="ඍ"/>
            <!-- U+0D94: "ඔ" SINHALA LETTER OYANNA -->
            <Key latin:keySpec="ඔ"/>
            <!-- U+0D9B: "ඛ" SINHALA LETTER MAHAAPRAANA KAYANNA -->
            <Key latin:keySpec="ඛ"/>
            <!-- U+0D9D: "ඝ" SINHALA LETTER MAHAAPRAANA GAYANNA -->
            <Key latin:keySpec="ඝ"/>
            <!-- U+0D9E: "ඞ" SINHALA LETTER KANTAJA NAASIKYAYA -->
            <Key latin:keySpec="ඞ"/>
            <!-- U+0DA1: "ඡ" SINHALA LETTER MAHAAPRAANA CAYANNA -->
            <Key latin:keySpec="ඡ"/>
            <!-- U+0DA3: "ඣ" SINHALA LETTER MAHAAPRAANA JAYANNA -->
            <Key latin:keySpec="ඣ"/>
            <!-- U+0DA5: "ඥ" SINHALA LETTER TAALUJA SANYOOGA NAAKSIKYAYA
                 U+0DF4: "෴" SINHALA PUNCTUATION KUNDDALIYA  -->
            <Key latin:keySpec="ඥ" latin:moreKeys="෴"/>
            <!-- U+0DA8: "ඨ" SINHALA LETTER MAHAAPRAANA TTAYANNA -->
            <Key latin:keySpec="ඨ"/>
        </case>
        <default>
            <!-- U+0D82: "ං" SINHALA SIGN ANUSVARAYA
                 U+0D83: "ඃ" SINHALA SIGN VISARGAYA -->
            <Key latin:keySpec="ං" latin:moreKeys="ඃ" latin:additionalMoreKeys="1" latin:keyHintLabel="1"/>
            <!-- U+0D85: "අ" SINHALA LETTER AYANNA -->
            <Key latin:additionalMoreKeys="2" latin:keyHintLabel="2" latin:keySpec="අ"/>
            <!-- U+0D89: "ඉ" SINHALA LETTER IYANNA -->
            <Key latin:keySpec="ඉ" latin:additionalMoreKeys="3" latin:keyHintLabel="3"/>
            <!-- U+0D8F: "ඏ" SINHALA LETTER ILUYANNA -->
            <Key latin:keySpec="ඏ" latin:additionalMoreKeys="4" latin:keyHintLabel="4"/>
            <!-- U+0D91: "එ" SINHALA LETTER EYANNA -->
            <Key latin:additionalMoreKeys="5" latin:keyHintLabel="5" latin:keySpec="එ"/>
            <!-- U+0D9A: "ක" SINHALA LETTER ALPAPRAANA KAYANNA -->
            <Key latin:keySpec="ක" latin:additionalMoreKeys="6" latin:keyHintLabel="6"/>
            <!-- U+0D9C: "ග" SINHALA LETTER ALPAPRAANA GAYANNA
                 U+0D9F: "ඟ" SINHALA LETTER SANYAKA GAYANNA -->
            <Key latin:keySpec="ග" latin:moreKeys="ඟ" latin:additionalMoreKeys="7" latin:keyHintLabel="7"/>
            <!-- U+0DA0: "ච" SINHALA LETTER ALPAPRAANA CAYANNA -->
            <Key latin:keySpec="ච" latin:additionalMoreKeys="8" latin:keyHintLabel="8"/>
            <!-- U+0DA2: "ජ" SINHALA LETTER ALPAPRAANA JAYANNA
                 U+0DA6: "ඦ" SINHALA LETTER SANYAKA JAYANNA -->
            <Key latin:keySpec="ජ" latin:moreKeys="ඦ" latin:additionalMoreKeys="9" latin:keyHintLabel="9"/>
            <!-- U+0DA4: "ඤ" SINHALA LETTER TAALUJA NAASIKYAYA
                 U+0DF4: "෴" SINHALA PUNCTUATION KUNDDALIYA -->
            <Key latin:keySpec="ඤ" latin:moreKeys="෴" latin:additionalMoreKeys="0" latin:keyHintLabel="0"/>
            <!-- U+0DA7: "ට" SINHALA LETTER ALPAPRAANA TTAYANNA -->
            <Key latin:keySpec="ට"/>
        </default>
    </switch>
</merge>
