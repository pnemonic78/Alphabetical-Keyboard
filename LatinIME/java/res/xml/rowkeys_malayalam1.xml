<merge xmlns:latin="http://schemas.android.com/apk/res-auto" >
    <!-- U+0D3E: "ാ" MALAYALAM VOWEL SIGN AA
         U+0D06: "ആ" MALAYALAM LETTER AA -->
    <Key latin:keySpec="ാ" latin:moreKeys="ആ,%" latin:additionalMoreKeys="1" latin:keyHintLabel="1"/>
    <!-- U+0D3F: "ി" MALAYALAM VOWEL SIGN I
         U+0D07: "ഇ" MALAYALAM LETTER I -->
    <Key latin:keySpec="ി" latin:moreKeys="ഇ,%" latin:additionalMoreKeys="2" latin:keyHintLabel="2"/>
    <!-- U+0D40: "ീ" MALAYALAM VOWEL SIGN II
         U+0D08: "ഈ" MALAYALAM LETTER II -->
    <Key latin:keySpec="ീ" latin:moreKeys="ഈ,%" latin:additionalMoreKeys="3" latin:keyHintLabel="3"/>
    <!-- U+0D41: "ു" MALAYALAM VOWEL SIGN U
         U+0D09: "ഉ" MALAYALAM LETTER U -->
    <Key latin:keySpec="ു" latin:moreKeys="ഉ,%" latin:additionalMoreKeys="4" latin:keyHintLabel="4"/>
    <!-- U+0D42: "ൂ" MALAYALAM VOWEL SIGN UU
         U+0D0A: "ഊ" MALAYALAM LETTER UU -->
    <Key latin:keySpec="ൂ" latin:moreKeys="ഊ,%" latin:additionalMoreKeys="5" latin:keyHintLabel="5"/>
    <!-- U+0D43: "ൃ" MALAYALAM VOWEL SIGN VOCALIC R
         U+0D0B: "ഋ" MALAYALAM LETTER VOCALIC R -->
    <Key latin:keySpec="ൃ" latin:moreKeys="ഋ,%" latin:additionalMoreKeys="6" latin:keyHintLabel="6"/>
    <!-- U+0D46: "െ" MALAYALAM VOWEL SIGN E
         U+0D0E: "എ" MALAYALAM LETTER E
         U+0D10: "ഐ" MALAYALAM LETTER AI
         U+0D48: "ൈ" MALAYALAM VOWEL SIGN AI -->
    <Key latin:keySpec="െ" latin:moreKeys="എ,ഐ,ൈ,%" latin:additionalMoreKeys="7" latin:keyHintLabel="7"/>
    <!-- U+0D47: "േ" MALAYALAM VOWEL SIGN EE
         U+0D0F: "ഏ" MALAYALAM LETTER EE -->
    <Key latin:keySpec="േ" latin:moreKeys="ഏ,%" latin:additionalMoreKeys="8" latin:keyHintLabel="8"/>
    <!-- U+0D4A: "ൊ" MALAYALAM VOWEL SIGN O
         U+0D12: "ഒ" MALAYALAM LETTER O -->
    <Key latin:keySpec="ൊ" latin:moreKeys="ഒ,%" latin:additionalMoreKeys="9" latin:keyHintLabel="9"/>
    <!-- U+0D4B: "ോ" MALAYALAM VOWEL SIGN OO
         U+0D13: "ഓ" MALAYALAM LETTER OO
         U+0D14: "ഔ" MALAYALAM LETTER AU
         U+0D57: "ൗ" MALAYALAM AU LENGTH MARK -->
    <Key latin:keySpec="ോ" latin:moreKeys="ഓ,ഔ,ൗ" latin:additionalMoreKeys="0" latin:keyHintLabel="0"/>
    <!-- U+0D4D: "്" MALAYALAM SIGN VIRAMA
         U+0D05: "അ" MALAYALAM LETTER A -->
    <Key latin:keySpec="്" latin:moreKeys="അ,%"/>
</merge>
