<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+0905: "अ" DEVANAGARI LETTER A
         U+094D: "्" DEVANAGARI SIGN VIRAMA
         U+0967: "१" DEVANAGARI DIGIT ONE -->
    <Key latin:keySpec="अ" latin:moreKeys="्" latin:additionalMoreKeys="१,1" latin:keyHintLabel="1"/>
    <!-- U+0906: "आ" DEVANAGARI LETTER AA
         U+093E: "ा" DEVANAGARI VOWEL SIGN AA
         U+0968: "२" DEVANAGARI DIGIT TWO -->
    <Key latin:keySpec="आ" latin:moreKeys="ा,%" latin:additionalMoreKeys="२,2" latin:keyHintLabel="2"/>
    <!-- U+0907: "इ" DEVANAGARI LETTER I
         U+093F: "ि" DEVANAGARI VOWEL SIGN I
         U+0969: "३" DEVANAGARI DIGIT THREE -->
    <Key latin:keySpec="इ" latin:moreKeys="ि" latin:additionalMoreKeys="३,3" latin:keyHintLabel="3"/>
    <!-- U+0908: "ई" DEVANAGARI LETTER II
         U+0940: "ी" DEVANAGARI VOWEL SIGN II
         U+096A: "४" DEVANAGARI DIGIT FOUR -->
    <Key latin:keySpec="ई" latin:moreKeys="ी,%" latin:additionalMoreKeys="४,4" latin:keyHintLabel="4"/>
    <!-- U+0909: "उ" DEVANAGARI LETTER U
         U+0941: "ु" DEVANAGARI VOWEL SIGN U
         U+096B: "५" DEVANAGARI DIGIT FIVE -->
    <Key latin:keySpec="उ" latin:moreKeys="ु" latin:additionalMoreKeys="५,5" latin:keyHintLabel="5"/>
    <!-- U+090A: "ऊ" DEVANAGARI LETTER UU
         U+0942: "ू" DEVANAGARI VOWEL SIGN UU
         U+096C: "६" DEVANAGARI DIGIT SIX -->
    <Key latin:keySpec="ऊ" latin:moreKeys="ू,%" latin:additionalMoreKeys="६,6" latin:keyHintLabel="6"/>
    <!-- U+090F: "ए" DEVANAGARI LETTER E
         U+0947: "े" DEVANAGARI VOWEL SIGN E
         U+096D: "७" DEVANAGARI DIGIT SEVEN -->
    <Key latin:keySpec="ए" latin:moreKeys="े" latin:additionalMoreKeys="७,7" latin:keyHintLabel="7"/>
    <!-- U+0910: "ऐ" DEVANAGARI LETTER AI
         U+0948: "ै" DEVANAGARI VOWEL SIGN AI
         U+096E: "८" DEVANAGARI DIGIT EIGHT -->
    <Key latin:keySpec="ऐ" latin:moreKeys="ै,%" latin:additionalMoreKeys="८,8" latin:keyHintLabel="8"/>
    <!-- U+0911: "ऑ" DEVANAGARI LETTER CANDRA O
         U+0949: "ॉ" DEVANAGARI VOWEL SIGN CANDRA O
         U+096F: "९" DEVANAGARI DIGIT NINE -->
    <Key latin:keySpec="ऑ" latin:moreKeys="ॉ" latin:additionalMoreKeys="९,9" latin:keyHintLabel="9"/>
    <!-- U+0913: "ओ" DEVANAGARI LETTER O
         U+094B: "ो" DEVANAGARI VOWEL SIGN O
         U+0966: "०" DEVANAGARI DIGIT ZERO -->
    <Key latin:keySpec="ओ" latin:moreKeys="ो" latin:additionalMoreKeys="०,0" latin:keyHintLabel="0"/>
    <!-- U+0914: "औ" DEVANAGARI LETTER AU
         U+094C: "ौ" DEVANAGARI VOWEL SIGN AU -->
    <Key latin:keySpec="औ" latin:moreKeys="ौ,%"/>
</merge>
