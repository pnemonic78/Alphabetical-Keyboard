<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+092C: "ब" DEVANAGARI LETTER BA
         U+092D: "भ" DEVANAGARI LETTER BHA -->
    <Key latin:keySpec="ब" latin:moreKeys="भ,%"/>
    <!-- U+092E: "म" DEVANAGARI LETTER MA
         U+0950: "ॐ" DEVANAGARI OM -->
    <Key latin:keySpec="म" latin:moreKeys="ॐ"/>
    <!-- U+092F: "य" DEVANAGARI LETTER YA -->
    <Key latin:keySpec="य"/>
    <!-- U+0930: "र" DEVANAGARI LETTER RA
         U+090B: "ऋ" DEVANAGARI LETTER VOCALIC R
         U+0943: "ृ" DEVANAGARI VOWEL SIGN VOCALIC R-->
    <Key latin:keySpec="र" latin:moreKeys="ऋ,ृ"/>
    <!-- U+0932: "ल" DEVANAGARI LETTER LA -->
    <Key latin:keySpec="ल"/>
    <!-- U+0935: "व" DEVANAGARI LETTER VA -->
    <Key latin:keySpec="व"/>
    <!-- U+0938: "स" DEVANAGARI LETTER SA
         U+0936: "श" DEVANAGARI LETTER SHA
         U+0937: "ष" DEVANAGARI LETTER SSA
         U+0936/U+094D/U+0930: "श्र" DEVANAGARI LETTER SHA/DEVANAGARI SIGN VIRAMA/DEVANAGARI LETTER RA -->
    <Key latin:keySpec="स" latin:moreKeys="श,ष,श्र"/>
    <!-- U+0939: "ह" DEVANAGARI LETTER HA -->
    <Key latin:keySpec="ह"/>
    <!-- U+0902: "ं" DEVANAGARI SIGN ANUSVARA
         U+0903: "ः‍" DEVANAGARI SIGN VISARGA
         U+0901: "ँ" DEVANAGARI SIGN CANDRABINDU
         U+093C: "़" DEVANAGARI SIGN NUKTA -->
    <Key latin:keySpec="ं" latin:moreKeys="ः,ँ,़"/>
    <!-- U+094D: "्" DEVANAGARI SIGN VIRAMA
         U+0945: "ॅ" DEVANAGARI VOWEL SIGN CANDRA E
         U+090D: "ऍ" DEVANAGARI LETTER CANDRA E -->
    <Key latin:keySpec="्" latin:moreKeys="ॅ,ऍ"/>
</merge>
