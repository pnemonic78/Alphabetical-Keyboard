<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <!-- U+0908: "ई" DEVANAGARI LETTER II
         U+0940: "ी" DEVANAGARI VOWEL SIGN II -->
    <Key latin:keySpec="ी" latin:moreKeys="ई,%"/>
    <!-- U+0909: "उ" DEVANAGARI LETTER U
         U+0941: "ु" DEVANAGARI VOWEL SIGN U -->
    <Key latin:keySpec="ु" latin:moreKeys="उ"/>
    <!-- U+090A: "ऊ" DEVANAGARI LETTER UU
         U+0942: "ू" DEVANAGARI VOWEL SIGN UU -->
    <Key latin:keySpec="ू" latin:moreKeys="ऊ,%"/>
    <!-- U+090D: "ऍ" DEVANAGARI LETTER CANDRA E
         U+0945: "ॅ" DEVANAGARI VOWEL SIGN CANDRA E -->
    <Key latin:keySpec="ॅ" latin:moreKeys="ऍ"/>
    <!-- U+090F: "ए" DEVANAGARI LETTER SHORT E
         U+0947: "े" DEVANAGARI VOWEL SIGN E -->
    <Key latin:keySpec="े" latin:moreKeys="ए"/>
    <!-- U+0948: "ै" DEVANAGARI VOWEL SIGN AI
         U+0910: "ऐ" DEVANAGARI LETTER AI -->
    <Key latin:keySpec="ै" latin:moreKeys="ऐ,%"/>
    <!-- U+0911: "ऑ" DEVANAGARI LETTER CANDRA O
         U+0949: "ॉ" DEVANAGARI VOWEL SIGN CANDRA O -->
    <Key latin:keySpec="ॉ" latin:moreKeys="ऑ"/>
    <!-- U+0913: "ओ" DEVANAGARI LETTER O
         U+094B: "ो" DEVANAGARI VOWEL SIGN O -->
    <Key latin:keySpec="ो" latin:moreKeys="ओ"/>
    <!-- U+094C: "ौ" DEVANAGARI VOWEL SIGN AU
         U+0914: "औ" DEVANAGARI LETTER AU -->
    <Key latin:keySpec="ौ" latin:moreKeys="औ,%"/>
    <!-- U+0905: "अ" DEVANAGARI LETTER A
         U+094D: "्" DEVANAGARI SIGN VIRAMA -->
    <Key latin:keySpec="्" latin:moreKeys="अ"/>
</merge>
