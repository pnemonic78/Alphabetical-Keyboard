<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <switch>
        <case
            latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
            >
            <!-- U+10B4: "ფ" GEORGIAN CAPITAL LETTER PHAR
                 U+10F6: "ჶ" GEORGIAN LETTER FI -->
            <Key
                latin:keyLabel="&#x10B4;"
                latin:moreKeys="&#x10F6;" />
            <!-- U+10B5: "ქ" GEORGIAN CAPITAL LETTER GHAN -->
            <Key
                latin:keyLabel="&#x10B5;" />
            <!-- U+10B6: "ღ" GEORGIAN CAPITAL LETTER GHAN -->
            <Key
                latin:keyLabel="&#x10B6;" />
            <!-- U+10B7: "ყ" GEORGIAN CAPITAL LETTER QAR
                 U+10F8: "ჸ" GEORGIAN LETTER ELIFI -->
            <Key
                latin:keyLabel="&#x10B7;"
                latin:moreKeys="&#x10F8;" />
            <!-- U+10B8: "შ" GEORGIAN CAPITAL LETTER SHIN -->
            <Key
                latin:keyLabel="&#x10B8;" />
            <!-- U+10B9: "ჩ" GEORGIAN CAPITAL LETTER CHIN
                 U+10BD: "ჭ" GEORGIAN CAPITAL LETTER CHAR -->
            <Key
                latin:keyLabel="&#x10B9;"
                latin:moreKeys="&#x10BD;" />
            <!-- U+10BA: "ც" GEORGIAN CAPITAL LETTER CAN
                 U+10BC: "წ" GEORGIAN CAPITAL LETTER CIL -->
            <Key
                latin:keyLabel="&#x10BA;"
                latin:moreKeys="&#x10BC;" />
        </case>
        <default>
            <!-- U+10E4: "ფ" GEORGIAN LETTER PHAR
                 U+10F6: "ჶ" GEORGIAN LETTER FI -->
            <Key
                latin:keyLabel="&#x10E4;"
                latin:moreKeys="&#x10F6;" />
            <!-- U+10E5: "ქ" GEORGIAN LETTER GHAN -->
            <Key
                latin:keyLabel="&#x10E5;" />
            <!-- U+10E6: "ღ" GEORGIAN LETTER GHAN -->
            <Key
                latin:keyLabel="&#x10E6;" />
            <!-- U+10E7: "ყ" GEORGIAN LETTER QAR
                 U+10F8: "ჸ" GEORGIAN LETTER ELIFI -->
            <Key
                latin:keyLabel="&#x10E7;"
                latin:moreKeys="&#x10F8;" />
            <!-- U+10E8: "შ" GEORGIAN LETTER SHIN -->
            <Key
                latin:keyLabel="&#x10E8;" />
            <!-- U+10E9: "ჩ" GEORGIAN LETTER CHIN
                 U+10ED: "ჭ" GEORGIAN LETTER CHAR -->
            <Key
                latin:keyLabel="&#x10E9;"
                latin:moreKeys="&#x10ED;" />
            <!-- U+10EA: "ც" GEORGIAN LETTER CAN
                 U+10EC: "წ" GEORGIAN LETTER CIL -->
            <Key
                latin:keyLabel="&#x10EA;"
                latin:moreKeys="&#x10EC;" />
        </default>
    </switch>
</merge>
